[Unit]
Description=Hook for ensuring sysmaint boots function as expected
Before=sysmaint-boot.target

[Service]
Type=oneshot
RemainAfterExit=yes
ExecStart=/usr/libexec/sysmaint-panel/sysmaint-boot

[Install]
WantedBy=sysmaint-boot.target
